<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

	<parameters>
		<parameter key="xnni_twig_extensions.twitter_bootstrap.class">Xnni\Bundle\TwitterBootstrapBundle\Twig\Extension\TwitterBootstrapExtension</parameter>
	</parameters>

	<services>
		<service id="twig.extension.xnni_twitter_bootstrap" class="%xnni_twig_extensions.twitter_bootstrap.class%">
			<tag name="twig.extension" />
		</service>
	</services>

</container>

